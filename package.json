{
  "name": "@blaqanalytics/n8n-nodes-blaq-list",
  "version": "1.0.0",
  "description": "n8n node for BLAQ List",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "n8n-nodes",
    "blaq list"
  ],
  "author": {
    "name": "BLAQ",
    "email": "support@blaqlistapp.com"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://blaqlistapp.com/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/blaqanalytics/n8n-nodes-blaq-list.git"
  },
  "packageManager": "pnpm@9.1.0",
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "dev:clean": "rm -rf node_modules/ && rm -rf dist/",
    "dev:link": "npm run build && npm link",
    "dev:n8n": "cd ~/.n8n/custom/ && npm link @blaqanalytics/n8n-nodes-blaq-list && n8n",
    "dev:publish": "npm publish --access public",
    "format": "prettier --write nodes credentials",
    "lint": "eslint -c eslint.config.cjs nodes credentials package.json",
    "lint:fix": "eslint -c eslint.config.cjs nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && eslint -c eslint.config.prepublish.cjs nodes credentials package.json"
  },
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BlaqListApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/BlaqList/BlaqList.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-plugin-n8n-nodes-base": "1.16.3",
    "jsonc-eslint-parser": "^2.4.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  }
}
